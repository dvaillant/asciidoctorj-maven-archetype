<archetype-descriptor xmlns="http://maven.apache.org/plugins/maven-archetype-plugin/archetype/1.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://maven.apache.org/plugins/maven-archetype-plugin/archetype/1.0.0 http://maven.apache.org/xsd/archetype-1.0.0.xsd" name="asciidoctorj-readme-archetype">
  <id>asciidoctorj-readme</id>
  <fileSets>
   <fileSet encoding="UTF8" >
     <directory></directory>
     <includes>
       <include>readme.adoc</include>
     </includes>
   </fileSet>
   <fileSet>
		<directory>src/main/resources/images</directory>
		<includes>
			<include>*.*</include>
		</includes>
	</fileSet>
  </fileSets>
</archetype-descriptor>
